{
  "name": "parsnip-ts",
  "version": "1.0.0",
  "description": "Parser combinators for TypeScript",
  "author": "Mike Cluck",
  "license": "MIT",
  "keywords": [],
  "main": "dist/lib.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "ts-node src/lib.ts",
    "watch": "ts-node-dev src/lib.ts",
    "dev": "npm run clean && npm run watch",
    "lint": "eslint . --ext .ts",
    "build:dev": "tsc --project tsconfig.json",
    "build:prod": "tsc --project tsconfig.prod.json",
    "clean": "rimraf dist/",
    "rebuild:dev": "npm run clean && npm run build:dev",
    "rebuild:prod": "npm run clean && npm run build:prod",
    "test": "jest --passWithNoTests",
    "test:watch": "npm test -- --watch",
    "prepublishOnly": "npm run rebuild:prod"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.7",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.13.0",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.5"
  }
}
